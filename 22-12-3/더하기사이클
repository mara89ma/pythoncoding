number = 26
sum = 0
num_list= []
count = 0
while True:
    last_digit = number%10 # 10으로 나눈 수
    num_list.insert(0,last_digit) # 마지막 수를 넣기
    number = number // 10 # number는 몫
    if number == 0: # number 가 0이되면 멈추기
        break
while True:
    for i in num_list:
        # print(i) # 분리한 수들을 하나씩 출력
        sum += i # 합구하기
    sum = sum%10
    # print(sum)
    if number == sum:
        break
    count += 1

# print(num_list)
# print(sum)
print(count)
